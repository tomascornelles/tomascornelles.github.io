<style webc:scoped>
  header {
    position: fixed;
    top: 0;
    inset-inline: 0;
    transition: all .4s ease;
    background-color: var(--color-bg);
    z-index: 1;
  }

  main {
    display: flex;
    align-items: top;
    justify-content: space-between;
  }

  main > * {
    flex-grow: 1;
    width: 100%;
    text-align: center;
  }

  main > *:first-child {
    text-align: left;
  }

  main > *:last-child {
    text-align: right;
  }

  h1 {
    margin: 0;
    font-size: 2rem;
    visibility: hidden;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
  }

  a {
    box-shadow: none;
  }

  nav span {
    margin: 0;
    font-size: 2rem;
  }

  nav a {
    display: block;
    padding: 1em;
  }

  nav a:hover {
    box-shadow: inset 0 -5em var(--color-fg);
  }

  .sticky h1 {
    visibility: visible;
  }

  #logo {
    transition: all .4s ease;
  }

  #logo path {
    fill: var(--color-fg);
  }

  .sticky #logo {
    height: calc(var(--unit) * 3);
  }

  nav ul {
    height: 0;
    overflow: hidden;
    list-style: none;
    padding: 0;
    margin: 3.4rem 0 0;
    position: absolute;
    left: 0;
    right: 0;
    background: var(--color-bg);
    transition: all .4s ease;
  }

  .sticky nav ul {
    margin-top: 0;
  }

  nav:hover ul {
    height: calc(3 * 3.5em); /* 4 elements, 1.5 line-height + 1 padding-top + 1 padding-bottom */
  }
</style>

<script webc:bucket="defer">
  window.onscroll = () => {
    const height = window.innerHeight;
    document.querySelector('header').classList.toggle('sticky', window.scrollY > height / 2);
  }
</script>
